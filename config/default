server
{
	listen			localhost
	server_name		webserv test	

	location / {
		autoindex		on
		#root			www
		limit_except	DELETE
	}

	location /www/
	{
		upload_path		/www/upload/
		autoindex on
		limit_except	GET POST
	}

	location /include/ {
		autoindex on
	}
}

server{
	listen					127.0.0.1:25565
	server_name				second_server lol thisiswebserv
	client_body_buffer_size	4200
	error_page				404	/error-404.html
}
