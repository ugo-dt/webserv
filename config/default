server
{
	listen			localhost:8080
	server_name		webserv.com test
	client_body_buffer_size	1000

	location / {
		cgi				.php	/cgi-bin/php
		autoindex		on
		limit_except	GET
	}

	location /www/
	{
		autoindex		on
		upload_path		/www/upload/
		rewrite			/www/redirect.html	/www/index.html
		default_file	index.html
		limit_except	GET POST
	}


	location /www/cgi-bin/ {
		autoindex		on
		limit_except	GET
	}

	location /www/cgi-bin/postCGI.cgi {
		cgi				.cgi	php-cgi
		limit_except	GET
	}


	location /www/upload/ {
		autoindex		on
		limit_except	GET POST DELETE
	}
}

server
{
	listen			localhost:8080
	server_name		listing.com

	location / {
		autoindex		on
	}
}

server{
	listen					127.0.0.1:1234
	server_name				second_server lol thisiswebserv
	client_body_buffer_size	4200
	error_page				404	/error-404.html
}
